Program.Sub.GeneratePurchaseOrderForm.Start

F.Intrinsic.Control.SetErrorHandler("GeneratePurchaseOrderForm_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sError.Declare(String)

V.Local.sSql.Declare
V.Local.sChgTimeStamp.Declare
V.Local.iBIRUNID.Declare
V.Local.iLOGID.Declare
V.Local.sAttach.Declare
V.Local.iQtyDec.Declare
V.Local.sCallParams.Declare
V.Local.sVendor.Declare
v.Local.sRet.Declare
v.Local.sReportID.Declare
V.Local.sPN.Declare
V.Local.sPV.Declare
V.Local.sParamName.Declare
V.Local.sParamVal.Declare
V.Local.iRet.Declare
V.Local.iLinkID.Declare
V.Local.iDocGrpID.Declare
V.Local.sDocGrpName.Declare
V.Local.bDocExists.Declare
V.Local.sFileName.Declare
V.Local.sFileNameWithoutExt.Declare
V.Local.sFileExtensionWithDot.Declare
V.Local.sFileExtension.Declare
V.Local.sTargetFileName.Declare
V.Local.sTargetFileNamePath.Declare
V.Local.bDirExists.Declare

'Query HIST_PO_LINE to get the latest PO Line Change Date & Time in String Format (YYYYMMDD_hhmmssss)
F.Intrinsic.String.Build("Select MAX(DATE_CHG + '_' + TIME_CHG) AS sPOLineChgTimeStamp from HIST_PO_LINE where PO = '{0}'",V.Global.sPO.Trim,V.Global.sSQL)
F.ODBC.Connection!conx.OpenLocalRecordsetRO("rstTimeStamp",V.Global.sSQL)
F.Intrinsic.Control.If(V.ODBC.conx!rstTimeStamp.EOF,=,False)
	V.Local.sChgTimeStamp.Set(V.ODBC.conx!rstTimeStamp.FieldValTrim!sPOLineChgTimeStamp)
	F.Intrinsic.Control.If(V.Local.sChgTimeStamp,<>,V.Args.sPOLineChgTimeStamp)
		
		F.Intrinsic.String.Build("SELECT DATE_ORDER, DATE_DUE, VENDOR FROM V_PO_HEADER WHERE PURCHASE_ORDER = '{0}'",V.Global.sPO.Trim,V.Local.sSql)
		F.ODBC.Connection!conx.OpenLocalRecordsetRO("rstPO",V.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.conx!rstPO.EOF,=,False)
			V.Local.sVendor.Set(V.ODBC.conx!rstPO.FieldValTrim!VENDOR)
		F.Intrinsic.Control.Else
			V.Local.sVendor.Set("")
		F.Intrinsic.Control.EndIf
		F.ODBC.conx!rstPO.Close
		
		'**************** Generate the BI Report for PO Form ***********************
		'getting run id and log id
		F.Global.BI.GetRunID(V.Local.iBIRUNID)
		F.Global.BI.StartLogging(V.Local.iBIRUNID,002801,-1,"",V.Local.iLOGID)
		
		'Set attachment filename and path
		F.Intrinsic.String.Build("{0}\GSS\{1}_{2}.pdf",V.System.Temp,V.Global.sPO.Trim,V.Local.sChgTimeStamp,V.Local.sAttach)
		
		'Adding value to QTYDEC param since the decimal values weren't being honored.
		'Get Decimal Quantity on PO (Company Options Adv. > Purchasing > Set Decimal Value)
		F.ODBC.Connection!conx.OpenLocalRecordsetRO("rst","Select (Case When F_LONG = 1 Then 4 When F_LONG = 2 Then 2  Else 0  End) AS QtyDec FROM OP_HEADER where ID = '400289'")
		F.Intrinsic.Control.If(V.ODBC.conx!rst.EOF,=,False)
			V.Local.iQtyDec.Set(V.ODBC.conx!rst.FieldValLong!QtyDec)
		F.Intrinsic.Control.EndIf
		F.ODBC.conx!rst.Close

		'Call GS program to build BI table data using Vendor Currency
		F.Intrinsic.String.Build("{0}!*!S!*! !*!N!*!Y",V.Global.sPO.Trim,V.Local.sCallParams)	
		F.Global.General.CallWrapperSyncBio(915000,V.Local.sCallParams)
		
		'verify that the table has the data we want.
		F.Intrinsic.String.Build("Select PO_NO From PRT_LASER_PO WHERE Terminal_NO = '{0}' AND PO_NO = '{1}'",v.Caller.Terminal,V.Global.sPO.Trim,v.Local.sSql) 
		F.ODBC.Connection!conx.ExecuteAndReturn(v.Local.sSql,v.Local.sRet)
		F.Intrinsic.Control.If(v.Local.sRet.Trim,=,"") 
			F.Intrinsic.UI.Msgbox("Unable to create purchase order data. Please try again. If issue persists please contact Global Shop Solutions for assistance.")
			F.Intrinsic.Control.ExitSub
		F.Intrinsic.Control.EndIf
		
		'Call BI Order Acknowledgement report
		'had to add in the qtydec param, otherwise cr is throwing up with missing parameter values...
		V.Local.sPN.Set("Terminal*!*ReportID*!*UseVendorCurr*!*PRTCNFRM*!*PROGRAM*!*PRTHIST*!*QTYDEC")
		
		'Check if the vendor has a custom report id associated to use it instead of default:2801
		F.Intrinsic.String.Build("SELECT BI_PO_RPT_ID FROM V_VENDOR_ADDL WHERE VENDOR = '{0}'",V.Local.sVendor,v.Local.sSql)
		F.ODBC.Connection!conx.OpenLocalRecordsetRO("rst",v.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.conx!rst.EOF,=,False)
			f.Intrinsic.Control.If(V.ODBC.conx!rst.FieldValTrim!BI_PO_RPT_ID,=,v.Ambient.Null,or,V.ODBC.conx!rst.FieldValTrim!BI_PO_RPT_ID,=,"000000",V.ODBC.conx!rst.FieldValTrim!BI_PO_RPT_ID,=,"0")
				'Set the default report id 
				v.Local.sReportID.Set("002801")	
			f.Intrinsic.Control.Else	
				'Set the custom report id for the vendor
				v.Local.sReportID.Set(V.ODBC.conx!rst.FieldValTrim!BI_PO_RPT_ID)
			f.Intrinsic.Control.EndIf
		F.Intrinsic.Control.EndIf
		
		'Use Vendor Currency
		F.Intrinsic.String.Build("{0}*!*{1}*!*Y*!* *!* *!* *!*{2}",V.Caller.Terminal,v.Local.sReportID,V.Local.iQtyDec,V.Local.sPV)
		
		F.Intrinsic.String.Split(V.Local.sPN,"*!*",V.Local.sParamName)
		F.Intrinsic.String.Split(V.Local.sPV,"*!*",V.Local.sParamVal)
		
		F.Global.BI.StopLogging(V.Local.iLOGID)
		
		'Creating the PO Form to PDF
		F.Global.BI.RunReportPreProcessor(V.Local.iBIRUNID,V.Local.iLOGID,V.Local.sPN,V.Local.sPV,"",4,True,"",-1,"",0,V.Local.sAttach,"",V.Local.iRet)
		
		'Get the Document Grp_ID & Grp_Name for 'PO REVISIONS'
		V.Local.sSql.Set("SELECT TOP 1 GRP_ID, GRP_NAME FROM ATG_DOC_GRP WHERE GRP_NAME = 'PO REVISIONS'")
		F.ODBC.Connection!conx.ExecuteAndReturn(v.Local.sSql,v.Local.sRet)
		F.Intrinsic.Control.If(v.Local.sRet.Trim,=,"") 
			V.Local.sSql.Set("INSERT INTO ATG_DOC_GRP(GRP_NAME, GRP_DESC) VALUES ('PO REVISIONS', 'PO REVISIONS')")
			F.ODBC.Connection!conx.Execute(V.Local.sSql)
						
			V.Local.sSql.Set("SELECT TOP 1 GRP_ID, GRP_NAME FROM ATG_DOC_GRP WHERE GRP_NAME = 'PO REVISIONS'")
			F.ODBC.Connection!conx.ExecuteAndReturn(v.Local.sSql,v.Local.sRet)
			F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
			V.Local.iDocGrpID.Set(V.Local.sRet(0))
			V.Local.sDocGrpName.Set(V.Local.sRet(1))
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
			V.Local.iDocGrpID.Set(V.Local.sRet(0))
			V.Local.sDocGrpName.Set(V.Local.sRet(1))
		F.Intrinsic.Control.EndIf
		
		'Attach the PO to Document Control
		F.Intrinsic.File.Exists(V.Local.sAttach,V.Local.bDocExists)
		F.Intrinsic.Control.If(V.Local.bDocExists,=,True)
			'Add Document Control Link because this file does, indeed, exist
			F.Intrinsic.File.GetFileNameFromFQN(V.Local.sAttach,V.Local.sFileName)
			F.Intrinsic.File.GetExtensionComponent(V.Local.sAttach,V.Local.sFileExtension)
			
			F.Intrinsic.String.Build(".{0}",V.Local.sFileExtension,V.Local.sFileExtensionWithDot)
			F.Intrinsic.String.Replace(V.Local.sFileName,V.Local.sFileExtensionWithDot,"",V.Local.sFileNameWithoutExt)
			
			'Create the destination filename
			F.Intrinsic.String.Build("\\{0}\Apps\LTI Attachments\PO REVISIONS\{1}",V.Ambient.GSSServer,V.Local.sFileName,V.Local.sTargetFileName)
			
			'Check whether the directory for the destination filename exists
			F.Intrinsic.File.GetPathFromFQN(V.Local.sTargetFileName,V.Local.sTargetFileNamePath)
			F.Intrinsic.File.DirExists(V.Local.sTargetFileNamePath,V.Local.bDirExists)
			F.Intrinsic.Control.If(V.Local.bDirExists,=,False)
				'Create the directory path
				F.Intrinsic.File.CreateDir(V.Local.sTargetFileNamePath)
			F.Intrinsic.Control.EndIf
			
			'Copy the attachment file to the Target Folder
			F.Intrinsic.File.CopyOpenFile(V.Local.sAttach,V.Local.sTargetFileName)
			
			'Create the LinkID
			F.Global.DocumentControl.CreateReference(V.Global.sPO.Trim,60,V.Local.iLinkID)		'Type ID = 60 is equivalent to PO

			'Add the Document
			F.Global.DocumentControl.AddDocument(V.Local.iLinkID,V.Local.sTargetFileName,V.Local.sFileNameWithoutExt,V.Local.iDocGrpID,V.Local.sFileExtension.UCase)
			F.Intrinsic.File.DeleteFile(V.Local.sAttach)
		F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
