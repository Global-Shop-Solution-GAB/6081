'Added by MH - 28 Apr 2016
V.Local.sSql.Declare

V.Local.sName.Declare


'Check if the PO does not have any lines after exiting PO header
V.Global.sPO.Set(V.Passed.000008.Trim)

F.Intrinsic.Control.If(V.Global.sPO.Trim,<>,"")
	F.Intrinsic.String.LPad(V.Global.sPO,"0",7,V.Global.sPO)
	F.Intrinsic.Control.CallSub(Sum_po_amount)
	F.Intrinsic.Control.CallSub(User_Amount_Limit)
	
	'F.Intrinsic.String.Concat("SELECT * FROM GAB_6081_APRVL WHERE purchase_order = '",V.Global.sPO.Trim,"' AND M_TYPE = 2",V.Global.sSQL)
	F.Intrinsic.String.Build("SELECT * FROM GAB_6081_APRVL WHERE purchase_order = '{0}'",V.Global.sPO.Trim,V.Global.sSQL)
	F.ODBC.Connection!conx.OpenLocalRecordsetRW("rst",V.Global.sSql)
	F.Intrinsic.Control.If(V.ODBC.conx!rst.EOF,=,False)
		F.Intrinsic.String.Build("SELECT * FROM V_PO_HEADER WHERE PURCHASE_ORDER = '{0}'",V.Global.sPO.Trim,V.Local.sSql)
		F.ODBC.Connection!conx.OpenLocalRecordsetRO("rstPO",V.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.conx!rstPO.EOF,=,True)
			F.ODBC.conx!rst.Delete
		'Update PO amount in approval table
		F.Intrinsic.Control.Else
			F.Intrinsic.Control.If(V.Caller.Switches,=,"N")
				F.Intrinsic.Control.CallSub(Sum_po_amount)
				F.ODBC.conx!rst.Set!AMOUNT(V.Args.Amount)
				
				F.Intrinsic.Control.If(V.Args.Amount,<=,V.Global.fLimit)
					F.Global.Security.GetFullName(V.Caller.User,V.Local.sName)
					F.ODBC.conx!rst.Set!Approver_Name(V.Local.sName.Trim)
					F.ODBC.conx!rst.Set!Approver(V.Caller.User.Trim)
					F.ODBC.conx!rst.Set!Approved_Date(V.Ambient.Now)
				F.Intrinsic.Control.Else
					F.ODBC.conx!rst.Set!Approver_Name("")
					F.ODBC.conx!rst.Set!Approver("")
					F.ODBC.conx!rst.Set!Approved_Date("1900-01-01")
					V.Global.fAmount.Set(V.Args.Amount)
					F.Intrinsic.Control.CallSub(Send_gssmsg)
				F.Intrinsic.Control.EndIf
			
				F.ODBC.conx!rst.Update
			F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.EndIf
		F.ODBC.conx!rstPO.Close
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Build("SELECT DATE_ORDER, DATE_DUE, VENDOR FROM V_PO_HEADER WHERE PURCHASE_ORDER = '{0}'",V.Global.sPO.Trim,V.Local.sSql)
		F.ODBC.Connection!conx.OpenLocalRecordsetRO("rstPO",V.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.conx!rstPO.EOF,=,False)
			F.ODBC.conx!rst.AddNew       
			F.ODBC.conx!rst.Set!purchase_order(V.Global.sPO.Trim)       
			F.ODBC.conx!rst.Set!VENDOR(V.ODBC.conx!rstPO.FieldValTrim!VENDOR)
			F.ODBC.conx!rst.Set!Amount(V.Args.Amount)
		'	F.Intrinsic.Control.CallSub(Format_date_vend)
			F.ODBC.conx!rst.Set!Date_ORDER(V.ODBC.conx!rstPO.FieldVal!DATE_ORDER)
			F.ODBC.conx!rst.Set!Date_Due(V.ODBC.conx!rstPO.FieldVal!DATE_DUE)
			F.ODBC.conx!rst.Set!Originator(V.Caller.User.Trim)
			F.Intrinsic.Control.If(V.Args.Amount,<=,V.Global.fLimit)
				F.Global.Security.GetFullName(V.Caller.User,V.Local.sName)
				F.ODBC.conx!rst.Set!Approver_Name(V.Local.sName.Trim)
				F.ODBC.conx!rst.Set!Approver(V.Caller.User.Trim)
				F.ODBC.conx!rst.Set!Approved_Date(V.Ambient.Now)
			F.Intrinsic.Control.Else
				F.ODBC.conx!rst.Set!Approver_Name("")
				F.ODBC.conx!rst.Set!Approver("")
				F.ODBC.conx!rst.Set!Approved_Date("1900-01-01")
				V.Global.fAmount.Set(V.Args.Amount)
				F.Intrinsic.Control.CallSub(Send_gssmsg)
			F.Intrinsic.Control.EndIf
			F.ODBC.conx!rst.Update
		F.Intrinsic.Control.EndIf
		F.ODBC.conx!rstPO.Close
	F.Intrinsic.Control.EndIf
	F.ODBC.conx!rst.Close
	
	F.Intrinsic.Control.CallSub("ReadPOLineChgTimeStamp")
	
	F.Intrinsic.Control.CallSub("GeneratePurchaseOrderForm","sPO",V.Global.sPO.Trim,"sPOLineChgTimeStamp",V.Args.sPOLineChgTimeStamp)
	
	F.Intrinsic.Control.CallSub("GetPOLineChgTimeStamp","sPO",V.Global.sPO.Trim)	

F.Intrinsic.Control.EndIf

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("Hook16860_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
	Function.Intrinsic.String.Concat("Project: GAB_6081_PO_APPROVAL_REQ.g2u",V.Ambient.Newline,V.Ambient.Newline,"Subroutine: ",V.Ambient.CurrentSubroutine,V.Ambient.NewLine,"Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.CallSub(Unload)
