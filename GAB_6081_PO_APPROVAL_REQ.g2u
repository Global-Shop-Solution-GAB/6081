	F.Intrinsic.Control.CallSub(Format_date_vend)
	F.ODBC.conx!rst.Set!Date_Due(V.Args.DateDue)
	F.Intrinsic.Control.If(V.Local.fAmount,<=,V.Global.fLimit)
